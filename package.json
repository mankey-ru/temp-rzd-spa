{
    "version": "1.0.0",
    "name": "sale-spa",
    "description": "Local environment for ticket sale system development",
    "author": "mankey-ru",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --env.nolimit=YES --hot --port 8090",
        "devw": "nodemon --watch webpack.config.js --exec \"npm run dev\"",
        "build-cab-s": "cross-env NODE_ENV=production webpack --progress --hide-modules --env.entrypoint=cabinet_special",
        "build-cab-m": "cross-env NODE_ENV=production webpack --progress --hide-modules --env.entrypoint=cabinet_mobile",
        "build-cab-r": "cross-env NODE_ENV=production webpack --progress --hide-modules --env.entrypoint=cabinet_redesign",
        "build-cab": "npm run build-cab-s && npm run build-cab-m && npm run build-cab-r"
    },
    "dependencies": {
        "underscore": "^1.8.3"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-loader": "^6.0.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-preset-es2015": "^6.0.0",
        "cross-env": "^3.1.4",
        "css-loader": "^0.25.0",
        "file-loader": "^0.9.0",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "less": "^2.7.2",
        "less-loader": "^3.0.0",
        "moment": "^2.19.1",
        "ssh-webpack-plugin": "^0.1.7",
        "style-loader": "^0.13.2",
        "superagent": "^3.7.0",
        "vue": "^2.5.2",
        "vue-loader": "^10.0.0",
        "vue-router": "^2.8.1",
        "vue-template-compiler": "^2.5.2",
        "vuex": "^2.5.0",
        "webpack": "^2.2.0",
        "webpack-dev-server": "^2.9.3"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    }
}
